{
  "name": "typescript-graphql",
  "license": "MIT",
  "scripts": {
    "prod": "npm -s run seed && cross-env NODE_ENV=production node dist/server.js",
    "seed": "ts-node prisma/seed.ts",
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/server",
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "build": "npm -s run generate && tsc",
    "build:prod": "npm -s run clean && npm -s run generate:prod && tsc",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prod": "npm -s run generate:prod:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate --schema=./prisma/schema.prisma",
    "generate:prod:prisma": "prisma generate --schema=./prisma/schema.prisma",
    "generate:nexus": "ts-node --transpile-only src/schema"
  },
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "@twurple/api": "^5.1.6",
    "@twurple/auth": "^5.1.6",
    "@twurple/auth-tmi": "^5.1.6",
    "@twurple/chat": "^5.1.6",
    "@twurple/pubsub": "^5.1.6",
    "apollo-server": "^3.6.7",
    "apollo-server-express": "^3.7.0",
    "axios": "^0.27.2",
    "connect-redis": "^6.1.3",
    "cron": "^2.0.0",
    "dotenv": "^16.0.1",
    "express-session": "^1.17.2",
    "graphql": "^16.4.0",
    "graphql-redis-subscriptions": "^2.4.2",
    "graphql-scalars": "^1.17.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.8.1",
    "ioredis": "^5.0.4",
    "nexus": "^1.3.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-twitch-strategy": "^2.2.0",
    "redis": "^4.1.0",
    "winston": "^3.7.2",
    "ws": "^8.6.0"
  },
  "devDependencies": {
    "@types/connect-redis": "^0.0.18",
    "@types/cron": "^2.0.0",
    "@types/express-session": "^1.17.4",
    "@types/node": "^17.0.29",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/ws": "^8.5.3",
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "mocha": "^10.0.0",
    "prisma": "^3.14.0",
    "supertest": "^6.2.3",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  },
  "engines": {
    "node": ">=16.3.0"
  }
}
